# AI Provider API Keys (add at least one)
VITE_OPENAI_API_KEY=your_openai_key_here
VITE_ANTHROPIC_API_KEY=your_anthropic_key_here
VITE_OPENROUTER_API_KEY=your_openrouter_key_here

# Optional: Configure default AI provider (openai | anthropic | openrouter)
VITE_DEFAULT_AI_PROVIDER=openai

# Optional: OpenRouter model selection (default: openai/gpt-4-turbo-preview)
# See https://openrouter.ai/docs#models for available models
VITE_OPENROUTER_MODEL=openai/gpt-4-turbo-preview
